{
	"publisher": "fedegasp",
	"name": "http-logger",
	"displayName": "Http Logger",
	"description": "Http Logger",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:http-logger.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configurationDefaults": {
			"http-logger.port": 9998
		},
		"commands": [
			{
				"command": "http-logger.helloWorld",
				"title": "Http Logger"
			}
		],
		"configuration": [
			{
				"title": "Http Logger",
				"properties": {
					"http-logger.port": {
						"type": "integer",
						"default": 9998,
						"description": "IP port"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
